<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Home of the Improbable.">
    <meta name="author" content="PhishyOne">
    <meta name="theme-color" content="#05899E">
    <meta name="theme-color" media="(prefers-color-scheme: dark)" content="#000000">

    <!-- Open Graph -->
    <meta property="og:title" content="Phishtopia">
    <meta property="og:description" content="Home of the Improbable.">
    <meta property="og:image" content="https://phishtopia.com/res/images/phishLogo.png">
    <meta property="og:url" content="https://phishtopia.com">
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="Phishtopia">

    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="Phishtopia">
    <meta name="twitter:description" content="Home of the Improbable.">
    <meta name="twitter:image" content="https://phishtopia.com/res/images/phishLogo.png">
    <meta name="twitter:site" content="@PhishyOne">
    <meta name="twitter:creator" content="@PhishyOne">

    <!-- Canonical -->
    <link rel="canonical" href="https://phishtopia.com">

    <!-- Title -->
    <title>Phishtopia</title>

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

    <!-- Preload primary font -->
    <link rel="preload" href="https://fonts.googleapis.com/css2?family=Cormorant+Garamond:wght@400&display=swap"
        as="style">

    <!-- All fonts -->
    <link
        href="https://fonts.googleapis.com/css2?family=Cormorant+Garamond:wght@300;400;500;600;700&family=Londrina+Shadow&family=Modak&family=Pompiere&display=swap"
        rel="stylesheet">

    <!-- Icons -->
    <link rel="apple-touch-icon" sizes="180x180" href="https://phishtopia.com/res/images/phishLogo.png">
    <link rel="icon" href="https://phishtopia.com/res/images/phishLogo.png" type="image/png">

    <!-- Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-LN+7fdVzj6u52u30Kp6M/trliBMCMKTyK833zpbD+pXdCLuTusPj697FH4R/5mcr" crossorigin="anonymous">

        <!-- Main Styles -->
        <link rel="stylesheet" href="./styles/main.css">
        
        <% if (typeof extraStyles !=='undefined' ) { %>
            <% if (Array.isArray(extraStyles)) { %>
                <% extraStyles.forEach(function(src) { %>
                    <link rel="stylesheet" href="<%= src %>">
                    <% }); %>
                        <% } else { %>
                            <link rel="stylesheet" href="<%= extraStyles %>">
                            <% } %>
                                <% } %>

</head>

